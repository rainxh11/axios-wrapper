{
  "name": "@rainxh11/axios-wrapper",
  "private": false,
  "version": "1.0.0",
  "description": "Axios Wrapper, which allows working with Axios to be less messy, more concise, clear, and functional.",
  "type": "module",
  "main": "dist/createAxios.js",
  "types": "dist/createAxios.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainxh11/axios-wrapper.git"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "axios",
    "functional",
    "fetch",
    "wrapper"
  ],
  "author": "Ahmed CHAKHOUM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rainxh11/axios-wrapper/issues"
  },
  "homepage": "https://github.com/rainxh11/axios-wrapper#readme",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf ./dist",
    "lint": "eslint --ext .ts,.js .",
    "format": "prettier \"./**\" --write --ignore-unknown"
  },
  "dependencies": {
    "axios": "^1.3.2",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
